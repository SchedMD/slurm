source ./globals

set matches     0

#
# Report the sinfo usage format
#

spawn $sinfo --usage
expect {
	-re "Usage: .*partition.*" {
		incr matches
		exp_continue
	}
	timeout {
		fail "sinfo not responding"
	}
	eof {
		wait
	}
}

if {$matches != 1} {
	fail "sinfo failed to report usage format"
}
