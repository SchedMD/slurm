source ./globals

set exit_code   0
set matches     0

#
# Report the sdiag version format
#

spawn $sdiag -V
expect {
	-re "slurm \[0-9]*.\[0-9]*.\[0-9]*" {
		incr matches
		exp_continue
	}
	timeout {
		log_error "sdiag not responding"
		set exit_code 1
	}
	eof {
		wait
	}
}

if {$matches != 1} {
	fail "sdiag -V failed ($matches)"
}

if {$exit_code != 0} {
	fail "Test failed due to previous errors (\$exit_code = $exit_code)"
}
