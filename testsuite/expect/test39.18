Name=gpu Type=1080ti File=${dev}\[0-2\] COREs=0,1
"

set fake_gpus_conf ""
set expected_output ""

set err_msgs $fs1_err
set expected_errs 3
check_configuration 1.6 $gres_conf $fake_gpus_conf $expected_output $err_msgs $expected_errs


