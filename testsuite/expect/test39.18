# ##############################################################################

set gres_conf "AutoDetect=nvml"
set fake_gpus_conf "
# This file was autogenerated by test$test_id
tesla|0|~zero|(null)|${dev}1
"
set expected_output ""
set err_msgs "fatal:"
set expected_errs 1
check_configuration 0.14 $gres_conf $fake_gpus_conf $expected_output $err_msgs $expected_errs

