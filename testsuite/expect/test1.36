source ./globals

#
# Spawn srun with $task_cnt tasks each of which runs a $mult way /bin/id
#
set mult         4
set task_output  0
set task_cnt 4
set node_cnt 1-4

set timeout $max_job_delay
spawn $srun --gres=craynetwork:0 -N$node_cnt -n$task_cnt -O -t1 --overlap $srun -l -n$mult -O --overlap $bin_id
expect {
	-re "($number): uid=" {
		incr task_output
		exp_continue
	}
	timeout {
		fail "srun not responding"
	}
	eof {
		wait
	}
}

if {$task_output != [expr $task_cnt * $mult]} {
	fail "Failed to get output from all tasks"
}
