source ./globals_accounting

set test_id     "21.26"
set exit_code   0
set tc1		[get_cluster_name]
set ta1		slurm_test-account.1
set ta2		slurm_test-account.2
set qos1	qqostest
set tu1         slurm_test-user.1
set access_err  0

print_header $test_id

set timeout 60

#
# Check accounting config and bail if not found
#
if { [test_account_storage] == 0 } {
	send_user "\nWARNING: This test can't be run without a usable AccountStorageType\n"
	exit 0
}

#
# Make sure we have permission to do this work
#
if { [string compare [check_accounting_admin_level] "Administrator"] } {
	send_user "\nWARNING: This test can't be run without being an Accounting administrator.\nUse sacctmgr mod user \$USER_NAME admin=admin.\n"
	exit 0
}

proc end_test { } {
	global tu1 ta1 ta2 tc1 qos1 qos_val
	set exit_code 0
	incr exit_code [remove_user "" "" "$tu1"]
	incr exit_code [remove_acct "" "$ta1,$ta2"]
	incr exit_code [remove_qos "$qos1"]
	if { [string length $qos_val] } {
		incr exit_code [mod_acct "$tc1" "" "root" "" "" "$qos_val" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
	}
	return $exit_code
}


#
# remove test associations to make sure we have a clean system
#
remove_user "" "" "$tu1"
remove_acct "" "$ta1,$ta2"
remove_qos "$qos1"

# get the qos of root on the cluster since we are going to set it to
# nothing here.  And put it back at the end of the test.
set match 0
set qos_val ""
set my_pid [eval spawn $sacctmgr list cluster $tc1 format=cluster,qos -np]
expect {
	-re "There was a problem" {
		send_user "FAILURE: there was a problem with the sacctmgr command\n"
		exit 1
	}
	-re "$tc1.(\[a-z,\]*)." {
		set qos_val $expect_out(1,string)
		set match 1
		exp_continue
	}

	timeout {
		send_user "\nFAILURE: sacctmgr list associations not responding\n"
		slow_kill $my_pid
		exit 1
	}
	eof {
		wait
	}
}
if { !$match } {
	send_user "\nFAILURE: couldn't query qos\n"
	exit 1
}

#now set default for cluster to ""
incr exit_code [mod_acct "$tc1" "" "root" "" "" " " "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	remove_qos "$qos1"
	exit $exit_code
}


#add qos
incr exit_code [add_qos "$qos1"]
if { $exit_code } {
	remove_qos "$qos1"
	exit $exit_code
}
#====Done Add QoS===========Begin Add First Account========
#add default account
incr exit_code [add_acct "$tc1" "" "$ta1" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}
#====Done Add First Account====Begin Add Second Account====
#add account
incr exit_code [add_acct "$tc1" "$ta1" "$ta2" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}
#=====Done Add Second Account========Begin Add User=========
#add user
incr exit_code [add_user "$tc1" "$ta1,$ta2" "$tu1" "" "" "$ta1" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}
#====Done add user====Done building test associations===Begin test section=======

# Verify initial associations
# NOTE: We do not use regular expressions below due to it's inability to test
# for a literal "|"
set matches 0
set my_pid [eval spawn $sacctmgr list assoc cluster=$tc1 format=qos,acct,user -p]
expect {
	-re "There was a problem" {
		send_user "FAILURE: there was a problem with the sacctmgr command\n"
		exit 1
	}
	"\n|root||" {
		incr matches
		exp_continue
	}
	"\n|root|root|" {
		incr matches
		exp_continue
	}
	"\n|$ta1||" {
		incr matches
		exp_continue
	}
	"\n|$ta1|$tu1|" {
		incr matches
		exp_continue
	}
	"\n|$ta2||" {
		incr matches
		exp_continue
	}
	"\n|$ta2|$tu1|" {
		incr matches
		exp_continue
	}
	timeout {
		send_user "\nFAILURE: sacctmgr list associations not responding\n"
		slow_kill $my_pid
		exit 1
	}
	eof {
		wait
	}
}
if {$matches != 6} {
	send_user "\nFAILURE: Initial sacctmgr add failed ($matches != 6)\n"
	end_test
	exit 1
}

#======Begin Modification to Add $qos1 to $ta1=======
#modify test1 account to add test QoS
incr exit_code [mod_acct "$tc1" "" "$ta1" "" "" "+$qos1" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}

#=======Done Modifying Account=======Next is Verify=======
set matches 0
set my_pid [eval spawn $sacctmgr list assoc cluster=$tc1 format=qos,acct,user -p]
expect {
	-re "There was a problem" {
		send_user "FAILURE: there was a problem with the sacctmgr command\n"
		exit 1
	}
	"\n|root||" {
		incr matches
		exp_continue
	}
	"\n|root|root|" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta1||" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta1|$tu1|" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta2||" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta2|$tu1|" {
		incr matches
		exp_continue
	}
	timeout {
		send_user "\nFAILURE: sacctmgr list associations not responding\n"
		slow_kill $my_pid
		exit 1
	}
	eof {
		wait
	}
}
if {$matches != 6} {
	send_user "\nFAILURE: failed on verify of +$qos1 to account $ta1 ($matches != 6)\n"
	end_test
	exit 1
}

#======Begin Modification to Subtract $qos1 from $account2=======
#modify test2 account to remove test QoS
incr exit_code [mod_acct "$tc1" "" "$ta2" "" "" "-$qos1" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}

#=======Done Modifying Account=======Next is Verify=======
set matches 0
set my_pid [eval spawn $sacctmgr list assoc cluster=$tc1 format=qos,acct,user -p]
expect {
	-re "There was a problem" {
		send_user "FAILURE: there was a problem with the sacctmgr command\n"
		exit 1
	}
	"\n|root||" {
		incr matches
		exp_continue
	}
	"\n|root|root|" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta1||" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta1|$tu1|" {
		incr matches
		exp_continue
	}
	"\n|$ta2||" {
		incr matches
		exp_continue
	}
	"\n|$ta2|$tu1|" {
		incr matches
		exp_continue
	}
	timeout {
		send_user "\nFAILURE: sacctmgr list associations not responding\n"
		slow_kill $my_pid
		exit 1
	}
	eof {
		wait
	}
}
if {$matches != 6} {
	send_user "\nFAILURE: verify of -$qos1 from account $ta2 ($matches != 6)\n"
	end_test
	exit 1
}


#======Begin Modification to Subtract $qos1 from $ta1=======
#modify test1 account to remove test QoS
incr exit_code [mod_acct "$tc1" "" "$ta1" "" "" "-$qos1" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}

#=======Done Modifying Account=======Next is Verify=======
set matches 0
set my_pid [eval spawn $sacctmgr list assoc cluster=$tc1 format=qos,acct,user -p]
expect {
	-re "There was a problem" {
		send_user "FAILURE: there was a problem with the sacctmgr command\n"
		exit 1
	}
	"\n|root||" {
		incr matches
		exp_continue
	}
	"\n|root|root|" {
		incr matches
		exp_continue
	}
	"\n|$ta1||" {
		incr matches
		exp_continue
	}
	"\n|$ta1|$tu1|" {
		incr matches
		exp_continue
	}
	"\n|$ta2||" {
		incr matches
		exp_continue
	}
	"\n|$ta2|$tu1|" {
		incr matches
		exp_continue
	}
	timeout {
		send_user "\nFAILURE: sacctmgr list associations not responding\n"
		slow_kill $my_pid
		exit 1
	}
	eof {
		wait
	}
}
if {$matches != 6} {
	send_user "\nFAILURE: verify of -$qos1 from account $ta1 ($matches != 6)\n"
	end_test
	exit 1
}

#=====Begin Modification to Add $qos1 from $ta2=======
#modify test2 account to add test QoS
incr exit_code [mod_acct "$tc1" "" "$ta2" "" "" "+$qos1" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}

#=======Done Modifying Account=======Next is Verify=======
set matches 0
set my_pid [eval spawn $sacctmgr list assoc cluster=$tc1 format=qos,acct,user -p]

expect {
	-re "There was a problem" {
		send_user "FAILURE: there was a problem with the sacctmgr command\n"
		exit 1
	}
	"\n|root||" {
		incr matches
		exp_continue
	}
	"\n|root|root|" {
		incr matches
		exp_continue
	}
	"\n|$ta1||" {
		incr matches
		exp_continue
	}
	"\n|$ta1|$tu1|" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta2||" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta2|$tu1|" {
		incr matches
		exp_continue
	}
	timeout {
		send_user "\nFAILURE: sacctmgr list associations not responding\n"
		slow_kill $my_pid
		exit 1
	}
	eof {
		wait
	}
}
if {$matches != 6} {
	send_user "\nFAILURE: failed on verify of +$qos1 to account $ta2 ($matches != 6)\n"
	end_test
	exit 1
}

#=======Begin Modification to Add $qos1 from $account2=======
#modify root account to add test QoS
incr exit_code [mod_acct "$tc1" "" "root" "" "" "+$qos1" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}

#=======Done Modifying Account=======Next Is Verify=======
set matches 0
set my_pid [eval spawn $sacctmgr list assoc cluster=$tc1 format=qos,acct,user -p]

expect {
	-re "There was a problem" {
		send_user "FAILURE: there was a problem with the sacctmgr command\n"
		exit 1
	}
	"\n$qos1|root||" {
		incr matches
		exp_continue
	}
	"\n$qos1|root|root|" {
		incr matches
		exp_continue
	}
	"\n|$ta1||" {
		incr matches
		exp_continue
	}
	"\n|$ta1|$tu1|" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta2||" {
		incr matches
		exp_continue
	}
	"\n$qos1|$ta2|$tu1|" {
		incr matches
		exp_continue
	}
	timeout {
		send_user "\nFAILURE: sacctmgr list associations not responding\n"
		slow_kill $my_pid
		exit 1
	}
	eof {
		wait
	}
}
if {$matches != 6} {
	send_user "\nFAILURE: failed on verify of +$qos1 to account root ($matches != 6)\n"
	end_test
	exit 1
}

#======Begin Modification to Add $qos1 from $account2=======
#modify test2 account to remove test QoS
incr exit_code [mod_acct "$tc1" "" "$ta2" "" "" "-$qos1" "" "" "" "" "" "" "" "" "" "" "" "" "" "" "" ""]
if { $exit_code } {
	end_test
	exit $exit_code
}

#=======Done Modifying Account=======Next is Verify=======
set matches 0
set my_pid [eval spawn $sacctmgr list assoc cluster=$tc1 format=qos,acct,user -p]

expect {
	-re "There was a problem" {
		send_user "FAILURE: there was a problem with the sacctmgr command\n"
		exit 1
	}
	"\n$qos1|root||" {
		incr matches
		exp_continue
	}
	"\n$qos1|root|root|" {
		incr matches
		exp_continue
	}
	"\n|$ta1||" {
		incr matches
		exp_continue
	}
	"\n|$ta1|$tu1|" {
		incr matches
		exp_continue
	}
	"\n|$ta2||" {
		incr matches
		exp_continue
	}
	"\n|$ta2|$tu1|" {
		incr matches
		exp_continue
	}
	timeout {
		send_user "\nFAILURE: sacctmgr list associations not responding\n"
		slow_kill $my_pid
		exit 1
	}
	eof {
		wait
	}
}
if {$matches != 6} {
	send_user "\nFAILURE: failed on verify of -$qos1 to account $ta2 ($matches != 6)\n"
	end_test
	exit 1
}

#======Done With List====== Ending=======
# This is the end below here
#
incr exit_code [end_test]
if {$exit_code == 0} {
	print_success $test_id
} else {
	send_user "\nFAILURE: $test_id\n"
}
exit $exit_code
