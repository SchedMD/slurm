proc cleanup {} {
	global tu1 tu2 tu3 tu4 ta1 ta2 ta3 ta4 tc1 file_in file_in2 file_in3

	_remove_user "" "$tu1,$tu2,$tu3,$tu4"
	_remove_acct "" "$ta1,$ta2,$ta3,$ta4"
	_remove_cluster "$tc1"

	#
	# Delete left-over input script files
	#
	file delete $file_in file_in2 file_in3
}

