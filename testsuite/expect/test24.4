		incr matches
		exp_continue
	}
	"AccountHTA|UHTAStd2|30|0.176471|10|0.047619|0.090909|0.699719|7595D4D300000000|0|0|" {
		incr matches
		exp_continue
	}
	"AccountHTA|UHTAStd3|50|0.294118|25|0.119048|0.227273|0.585310|7595B24300000000|0|0|" {
		incr matches
		exp_continue
	}

	timeout {
		send_user "\nFAILURE: spawn IO not responding\n"
		cancel_job $job_id
		set exit_code 1
	}
	eof {
		wait
	}
}

if {$matches != 22} {
	send_user "\nFAILURE: we didn't get the correct priorities from the plugin ($matches != 13)\n"
	set exit_code 1
}

if {$exit_code == 0} {
	file delete $test_prog
	send_user "\nSUCCESS\n"
}
exit $exit_code
