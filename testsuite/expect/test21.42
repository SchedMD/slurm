source ./globals

set exit_code   0

#
# Report the sacctmgr usage format
#

proc test_header_in_format { entity } {
	global sacctmgr exit_code
	log_info "Verifying sacctmgr show $entity format="
	log_user 0
	spawn $sacctmgr show $entity -p
	expect {
		-re "(\[a-zA-Z| \]+)" {
			set names [ split $expect_out(buffer) "|" ]
			foreach name $names {
				if { $name != "" } {
					spawn $sacctmgr show $entity format=$name
					expect {
						-re "Unknown field" {
							log_error "Failed to validate use of $name in sacctmgr show $entity format."
							set exit_code 1
						}
					}
				}
			}
		}
	log_user 1
	}
}

test_header_in_format "qos"


if {$exit_code != 0} {
	fail "Test failed due to previous errors (\$exit_code = $exit_code)"
}
