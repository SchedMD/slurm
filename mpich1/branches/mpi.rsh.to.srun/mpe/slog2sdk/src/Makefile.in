MAKE=@MAKE@
RM=@RM@
SHELL=/bin/sh

TARGETS = @slog2_make_dirs@


all:
	@for dir in $(TARGETS) ; do \
	    ( cd $$dir && $(MAKE) ) ; \
	 done

clean:
	@for dir in $(TARGETS) ; do \
	    ( cd $$dir && $(MAKE) clean ) ; \
	 done

rmlib:
	@for dir in $(TARGETS) ; do \
	    ( cd $$dir && $(MAKE) rmlib ) ; \
	 done

realclean:
	@for dir in $(TARGETS) ; do \
	    ( cd $$dir && $(MAKE) realclean ) ; \
	 done

distclean-misc:
	@-$(RM) Makefile
	@-$(RM) *.manifest
	@-$(RM) config.log config.status
	@-$(RM) -r autom4te*.cache

distclean: distclean-misc
	@-for dir in $(TARGETS) ; do \
	     ( cd $$dir && $(MAKE) distclean ) ; \
	  done

maintainer-clean: distclean

owner-clean: maintainer-clean
	@-$(RM) configure
