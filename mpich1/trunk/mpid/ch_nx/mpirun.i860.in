#! /bin/sh
exitstatus=1
if [ "$argsset" = "" ] ; then
   . $MPIRUN_HOME/mpirun.args
   argsset=1
fi

exitstatus=0
if [ $just_testing = 1 ] ; then
  echo "getcube -t $np"
  echo "load $progname $cmdLineArgs"
  echo "waitcube"
  echo "relcube"
else
  getcube -t $np
  if [ $? != 0 ] ; then
	 echo "Requested number of nodes not available"
  else
     load $progname $cmdLineArgs
     waitcube
     relcube
  fi
fi
