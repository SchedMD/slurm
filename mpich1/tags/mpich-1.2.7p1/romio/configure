ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS top_build_dir docdir htmldir RANLIB ac_ct_RANLIB CC F77 CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT FFLAGS ac_ct_F77 CPP EGREP F90 HAVE_WEAK_SYMBOLS srcdir master_top_srcdir master_top_builddir master_topbuild_dir MPIO_EXTRA_OBJECTS MPIO_EXTRA_TMP_POBJECTS MPIO_EXTRA_REAL_POBJECTS MPIO_REQOBJECTS MPIO_REQ_TMP_POBJECTS MPIO_REQ_REAL_POBJECTS DOCTEXT EXTRA_SRC_DIRS FILE_SYS_DIRS GLUE_DIR VPATH ARCH FILE_SYSTEM USER_CFLAGS USER_FFLAGS MIPS BITS MPI AR MPI_INCLUDE_DIR MPI_LIB NOF77 NOPROFILE MAKE arch_IRIX ROMIO_HOME LIBNAME TEST_LIBNAME LL F77GETARG F77IARGC F77MPIOINC FTESTDEFINE FORTRAN_MPI_OFFSET FROM_MPICH FROM_MPICH2 FROM_LAM WITHIN_KNOWN_MPI_IMPL NEEDS_MPI_FINT HAVE_MPI_INFO BUILD_MPI_INFO HAVE_MPI_DARRAY_SUBARRAY BUILD_MPI_ARRAY DEFINE_MPI_OFFSET DEFINE_HAVE_MPI_GREQUEST MPI_OFFSET_TYPE MPI_FINFO1 MPI_FINFO2 MPI_FINFO3 MPI_FINFO4 MPI_FARRAY1 MPI_FARRAY2 MPI_FARRAY3 MPI_FARRAY4 MPI_FARRAY5 MPI_FARRAY6 MPI_FARRAY7 MPI_OFFSET_KIND1 MPI_OFFSET_KIND2 TEST_CC TEST_F77 ROMIO_INCLUDE ROMIO_LFLAGS ROMIO_LIBLIST ROMIO_TCFLAGS ROMIO_TCPPFLAGS ROMIO_TFFLAGS MPIRUN FORTRAN_TEST MAKE_DEPEND_C ENABLE_SHLIB CC_SHL LIBTOOL SHLIBNAME LIBOBJS LTLIBOBJS'
