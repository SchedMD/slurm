rm -f *.o

if test $FROM_MPICH = no -a $FROM_MPICH2 = no ; then
   echo "$as_me:$LINENO: result: Configure done. Now type make." >&5
echo "${ECHO_T}Configure done. Now type make." >&6
fi
exit 0
